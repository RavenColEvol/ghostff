{
  "name": "@lamkoti/ghostff-cli",
  "version": "1.0.4",
  "description": "Command line interface for fetching data from contentstack delivery SDK",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "bin": {
    "ghostff": "./dist/esm/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "npm run build && tsc --watch -p tsconfig.esm.json",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "contentstack",
    "cli",
    "cms"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lamkoti/ghostff-core": "^1.0.3",
    "chalk": "^5.0.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@contentstack/delivery-sdk": "^4.8.0",
    "@types/dotenv": "^8.2.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
